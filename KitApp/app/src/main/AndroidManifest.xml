        android:theme="@style/Theme.KitApp">
        <activity android:name=".activity.UyeListesiActivity" />
        <activity android:name=".activity.KitapOnerilerimActivity" />
        <activity android:name=".activity.UyeProfilActivity" />
        <activity android:name=".activity.YoneticiGirisActivity" />
        <activity android:name=".activity.UyeOlActivity" />
        <activity android:name=".activity.DetayActivity" />
